<div class="row">
    <div class="span8">
        <% news = @getCollection("news").toJSON() %>
        <% for item, i in news: %>
        <div class="paper">
            <div class="row">
                <div class="span2">
                    <p><em><%- @formatDate(item.date) %></em></p>
                </div>
                <div class="span6">
                    <%- item.contentRendered %>
                </div>
            </div>
        </div>
        <% end %>
    </div>
</div>
